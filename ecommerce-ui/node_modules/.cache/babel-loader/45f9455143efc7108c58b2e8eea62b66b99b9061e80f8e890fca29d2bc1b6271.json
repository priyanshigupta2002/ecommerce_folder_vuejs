{"ast":null,"code":"import NavbarComponent from \"./components/NavbarComponent.vue\";\nimport axios from 'axios';\nexport default {\n  components: {\n    NavbarComponent\n  },\n  data() {\n    return {\n      baseURL: \"https://limitless-lake-55070.herokuapp.com/\",\n      products: [],\n      categories: []\n    };\n  },\n  methods: {\n    async fetchData() {\n      // api call to get all the categories\n      await axios.get(this.baseURL + \"category/\").then(res => {\n        this.categories = res.data;\n      }).catch(err => console.log('err', err));\n\n      // api call to get the products\n\n      await axios.get(this.baseURL + \"product/\").then(res => {\n        this.products = res.data;\n      }).catch(err => console.log('err', err));\n    }\n  },\n  mounted() {\n    this.fetchData();\n  }\n};","map":{"version":3,"names":["NavbarComponent","axios","components","data","baseURL","products","categories","methods","fetchData","get","then","res","catch","err","console","log","mounted"],"sources":["/home/priyanshi/Documents/Learning Vue JS/ecommerce_yt_git_folder/ecommerce-ui/src/App.vue"],"sourcesContent":["<template>\n  <div>\n  <NavbarComponent />\n  <div id=\"nav\">\n    <router-link to=\"/\">Home</router-link> |\n    <router-link to=\"/about\">About</router-link>\n  </div>\n  <router-view\n  :baseURL=\"baseURL\"\n  :categories=\"categories\"\n  :products=\"products\"\n  >\n  </router-view>\n  </div>\n</template>\n\n<script>\nimport NavbarComponent from \"./components/NavbarComponent.vue\";\nimport axios from 'axios';\nexport default {\n  components: { NavbarComponent },\n  data() {\n    return {\n      baseURL : \"https://limitless-lake-55070.herokuapp.com/\",\n      products: [],\n      categories: []\n    }\n  },\n  methods: {\n    async fetchData() {\n\n      // api call to get all the categories\n      await axios.get(this.baseURL + \"category/\")\n      .then(res => {\n        this.categories = res.data\n      }).catch((err) => console.log('err', err));\n\n      // api call to get the products\n\n      await axios.get(this.baseURL + \"product/\")\n      .then(res => {\n        this.products = res.data\n      }).catch((err) => console.log('err', err));\n    }\n  },\n  mounted() {\n    this.fetchData();\n  }\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n\n#nav {\n  padding: 30px;\n}\n\n#nav a {\n  font-weight: bold;\n  color: #2c3e50;\n}\n\n#nav a.router-link-exact-active {\n  color: #42b983;\n}\n</style>\nNavbarComponent"],"mappings":"AAiBA,OAAOA,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,UAAU,EAAE;IAAEF;EAAgB,CAAC;EAC/BG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAM,EAAI,6CAA6C;MACvDC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IACd;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,SAASA,CAAA,EAAG;MAEhB;MACA,MAAMP,KAAK,CAACQ,GAAG,CAAC,IAAI,CAACL,OAAM,GAAI,WAAW,EACzCM,IAAI,CAACC,GAAE,IAAK;QACX,IAAI,CAACL,UAAS,GAAIK,GAAG,CAACR,IAAG;MAC3B,CAAC,CAAC,CAACS,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,GAAG,CAAC,CAAC;;MAE1C;;MAEA,MAAMZ,KAAK,CAACQ,GAAG,CAAC,IAAI,CAACL,OAAM,GAAI,UAAU,EACxCM,IAAI,CAACC,GAAE,IAAK;QACX,IAAI,CAACN,QAAO,GAAIM,GAAG,CAACR,IAAG;MACzB,CAAC,CAAC,CAACS,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,GAAG,CAAC,CAAC;IAC5C;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR,IAAI,CAACR,SAAS,CAAC,CAAC;EAClB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}